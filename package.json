{
  "name": "aws-git-backed-gatsby-static-site",
  "version": "0.8.2",
  "author": "Adam Trimble",
  "description": "An easy cloudformation template designed to launch gatsby static sites cheaply and securely",
  "main": "index.js",
  "scripts": {
    "create-stack": "export $(cat config.env | xargs) && ./bin/create.sh",
    "delete-stack": "export $(cat config.env | xargs) && ./bin/delete.sh",
    "update-stack": "export $(cat config.env | xargs) && ./bin/update.sh"
  },
  "keywords": ["gatsby", "static site", "static site generators", "AWS", "serverless"],
  "license": "MIT"
}
